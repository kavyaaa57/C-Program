#include <stdio.h>

int main() {
    int a = 3, b = 3, c = 3;
    if (a > 0 && b > 0 && c > 0) {
        if (a == b && b == c)
            printf("Equilateral triangle");
        else if (a + b > c && b + c > a && c + a > b)
            printf("Valid Triangle");
        else
            printf("Not a triangle");
    } else {
        printf("Not a triangle");
    }
    return 0;
}
